{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "max-len": ["error", { "code": 100 }]
    }
}
